var x, d, R, i: longint;
begin
    for i := 1 to 1000000 do
    begin
      x := i;
      R := 0;
      while x > 0 do begin
        d := x mod 10;
            R := 10*R + d;
            x := x div 10
      end;
      
      if (R < 100) and (R > 9) and ((R mod 10) + (R div 10) = 16) then
      begin
        writeln(i);
        break;
      end;
    end;
end.
