// 2 nomer olimpiadi

#include <iostream>
using namespace std;

int Kreplenie(int a, int b, int c)
{
    if ((a < b < c) && (c < b < a))
        return b;
    else if (b < a < c) && (c < a < b)
        return a;
    else
        return c;
}

int main()
{
    int a, b, c;
    cin >> a >> b >> c;
    
    cout << Kreplenie(a, b, c) << endl;
    return 0;
}
