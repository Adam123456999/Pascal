var x, a, b, i: longint;
begin
    for i := 1 to 1000000 do
    begin
      x := i;
      a := 0; b := 0;
      while x > 0 do begin
        if x mod 2 > 0 then
            a := a + 1
        else
            b := b + x mod 6;
        x := x div 6;
      end;
      
      if (a = 2) and (b = 8) then
      begin
        writeln(i);
        break;
      end;
    end;
end.
