var x, L, M, i: longint;
begin
    for i := 1 to 1000000 do
    begin
      x := i;
      L:=0; M:=0;
      while x > 0 do begin
        M := M + 1;
        if x mod 2 <> 0
            then L := L + 1;
        x := x div 2;
      end;
      
      if (L = 5) and (M = 6) then
      begin
        writeln(i);
        break;
      end;
    end;
end.
