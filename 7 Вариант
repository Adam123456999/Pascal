var x, a, b, i: longint;
begin
    for i := 1 to 1000000 do
    begin
      x := i;
      a := 0; b := 1;
      while x > 0 do begin
        if x mod 2 > 0 then
            a := a + x mod 12
        else
            b := b * (x mod 12);
        x := x div 12;
      end;
      
      if (a = 3) and (b = 12) then
      begin
        writeln(i);
        break;
      end;
    end;
end.
