var x, a, b, c, i: longint;
begin
    for i := 1 to 1000000 do
    begin
      x := i;
      a := 0; b := 0;
      while x > 0 do begin
        c := x mod 2;
            if c = 0 then
                a := a + 1
            else
                b := b + 1;
            x := x div 10
      end;
      
      if (a = 2) and (b = 3) then
      begin
        writeln(i);
        break;
      end;
    end;
end.
