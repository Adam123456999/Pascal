#include <iostream>
#include <fstream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");

	fstream file("26.txt");
	string s;
	int size;
	file >> size;
	vector<int> v(size);
	for (int i = 0; i < size; i++) {
		file >> s;
		v[i] = stoi(s);
	}

	file.close();

	vector<int> arif;
	for (int i = 0; i < size - 1; i++) {
		for (int j = i + 1; j < size; j++) {
			if ((v[i] % 2 == 0) && (v[j] % 2 == 0))
				if (find(v.begin(), v.end(), ((v[i] + v[j]) / 2)) != v.end())
					arif.push_back((v[i] + v[j]) / 2);
		}
	}
	
	v.resize(0);

	sort(arif.begin(), arif.end());

	cout << arif.size() << " " << arif[arif.size() - 1] << endl;
	

	
	cin.get();
	return 0;
}
